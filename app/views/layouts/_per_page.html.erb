<ul class="pagination pull-right">
  <li class="disabled"><span>Per Page</span></li>
  <% [10,25,50].each do |count| %>
    <li class="<%= 'active' if current_user.send(type+"_per_page") == count %> per_page"><%= link_to count, "#", 'data-count' => count, 'data-object' => type, id: "count_#{count}" %></li>
  <% end %>
  <% if defined?(object_count) and object_count %><li class="disabled"><span>out of <%= object_count %></span></li><% end %>
</ul>

<div class="clear">&nbsp;</div>
